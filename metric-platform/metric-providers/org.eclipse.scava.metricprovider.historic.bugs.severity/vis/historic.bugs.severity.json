{
	"metricid" : "org.eclipse.scava.metricprovider.historic.bugs.severity.SeverityHistoricMetricProvider",
	"vis" : [ 
		{
			"id" : "bugs.severity",
			"name" : "Bug Tracker Severity",
			"description" : "The number of bugs per severity level",
			"type" : "LineChart",
			"datatable" : {
				"rows" : "$severityLevels",
				"cols" : [  { "name" : "Date", "field" : "$__date" }, 
					{ "name" : "Severity", "field" : "$severityLevel" },
					{ "name" : "Bugs", "field" : "$numberOfBugs" } ]
			},
			"x" : "Date",
			"y" : "Bugs", 
			"series" : "Severity",
			"timeSeries" : true
		},
		{
			"id" : "bugs.severity.percentages",
			"name" : "Bug Tracker Severity (Percentages)",
			"description" : "The percentage of bugs per severity level",
			"type" : "LineChart",
			"datatable" : {
				"rows" : "$severityLevels",
				"cols" : [  { "name" : "Date", "field" : "$__date" }, 
					{ "name" : "Severity", "field" : "$severityLevel" },
					{ "name" : "Bugs", "field" : "$percentage" } ]
			},
			"x" : "Date",
			"y" : "Bugs", 
			"series" : "Severity",
			"timeSeries" : true
		}
	]
}
