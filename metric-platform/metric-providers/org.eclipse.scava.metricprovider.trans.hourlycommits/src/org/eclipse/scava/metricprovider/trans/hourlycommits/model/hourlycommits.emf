package org.eclipse.scava.metricprovider.trans.hourlycommits.model;

@viz(target="gcharts", chart="bar")
@db(qualifiedCollectionNames="true")
class HourlyCommits {
	@series(label="hour")
	val Hour[*] hours;
}

class Hour {
	@searchable
	attr String hour;
	@yaxis(title="Number of commits")
	attr int numberOfCommits;
}