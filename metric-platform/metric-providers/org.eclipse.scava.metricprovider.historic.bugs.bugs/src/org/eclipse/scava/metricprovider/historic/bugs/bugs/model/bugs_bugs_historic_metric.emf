package org.eclipse.scava.metricprovider.historic.bugs.bugs.model;

class BugsBugsHistoricMetric {
	val DailyBugTrackerData[*] bugTrackers;
	attr int numberOfBugs;
	
	attr float averageCommentsPerBug;
	attr float averageRequestsPerBug;
	attr float averageRepliesPerBug;
	
	attr float averageCommentsPerUser;
	attr float averageRequestsPerUser;
	attr float averageRepliesPerUser;
