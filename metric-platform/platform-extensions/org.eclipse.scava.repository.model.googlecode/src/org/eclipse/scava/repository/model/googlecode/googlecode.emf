@namespace(uri="org.eclipse.scava.repository.model.googlecode", prefix="org.eclipse.scava.repository.model.googlecode")
package org.eclipse.scava.repository.model.googlecode;

@stub(javaClass="org.eclipse.scava.repository.model.Project")
class Project {
}

@stub(javaClass="org.eclipse.scava.repository.model.NamedElement")
class NamedElement {
}

@stub(javaClass="org.eclipse.scava.repository.model.CommunicationChannel")
class CommunicationChannel {
}

@stub(javaClass="org.eclipse.scava.repository.model.Person")
class Person {
}

@stub(javaClass="org.eclipse.scava.repository.model.BugTrackingSystem")
class BugTrackingSystem {
}

class GoogleCodeProject extends Project {
  val GoogleWiki wiki;
  val GoogleForum forum;
  val GoogleDownload[*] downloads;
  val GoogleIssueTracker issueTracker;
  attr Integer stars = "0";
}

class GoogleWiki extends CommunicationChannel {
}

class GoogleForum extends CommunicationChannel {
}

@customize
class GoogleIssueTracker extends BugTrackingSystem {
  attr String url;
  val GoogleIssue[*] issues;
}

class GoogleIssue {
  val GoogleUser[1] owner;
  attr String created_at;
  attr String updated_at;
  attr String priority;
  attr String type;
  attr String component;
  attr String status;
  val GoogleIssueComment[*] comments;
  val GoogleLabel[*] labels;
  attr Integer stars;
  attr String summary;
}

