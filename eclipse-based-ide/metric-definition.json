  "basics": {
    "scava-lib-usage": {
      "human-readable": "Level of using CROSSMINER library change function",
      "description": "Ratio between user or CROSSMINER plug-in executed library changes."
    },
    "scava-search-usage": {
      "human-readable": "Level of using CROSSMINER search function",
      "description": "Count of time when the user initiate any free text (keyword) search using CROSSMINER plug-in."
    },
    "scava-search-success": {
      "human-readable": "Level of successfull free text serach using CROSSMINER plug-in",
      "description": "Count of time when the user initiate any free text (keyword) search using CROSSMINER plug-in divided by the ratio between user or CROSSMINER plug-in executed library changes."
    },
    "modifictaion-rate": {
      "human-readable": "Rate of changes applied to document",
      "description": "Count of DocumentEvents, which denote content changes of various source files."
    },
    "gui-usage-rate": {
