      </super>
   </extension>
   <extension
         id="org.eclipse.scava.plugin.markers.apimigrationdetection"
         name="API migration code element"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <attribute
            name="org.eclipse.scava.plugin.markers.apimigrationdetection.detectiondata">
      </attribute>
   </extension>
   <extension
         point="org.eclipse.ui.editors.markerAnnotationSpecification">
      <specification
            annotationType="org.eclipse.scava.plugin.annotations.sourcecodehighlight"
            colorPreferenceKey="org.eclipse.scava.plugin.annotations.sourcecodehighlight.color"
            colorPreferenceValue="255,108,211"
            contributesToHeader="true"
            highlightPreferenceKey="org.eclipse.scava.plugin.annotations.sourcecodehighlight.highlight"
            highlightPreferenceValue="true"
            icon="icons/icon-16-white.png"
            includeOnPreferencePage="true"
            label="CROSSMINER Source Code Highlight"
            overviewRulerPreferenceKey="org.eclipse.scava.plugin.annotations.sourcecodehighlight.overview"
            overviewRulerPreferenceValue="true"
            presentationLayer="10000"
            quickFixIcon="icons/icon-16-white.png"
            showInNextPrevDropdownToolbarActionKey="org.eclipse.scava.plugin.annotations.sourcecodehighlight.nextprev"
            textPreferenceKey="org.eclipse.scava.plugin.annotations.sourcecodehighlight.text"
            textPreferenceValue="true"
            textStylePreferenceValue="BOX"
            verticalRulerPreferenceKey="org.eclipse.scava.plugin.annotations.sourcecodehighlight.ruler"
            verticalRulerPreferenceValue="true">
      </specification>
      <specification
            annotationType="org.eclipse.scava.plugin.annotations.apimigrationdetection"
            colorPreferenceKey="org.eclipse.scava.plugin.annotations.apimigrationdetection.color"
            colorPreferenceValue="255,108,211"
            contributesToHeader="true"
            highlightPreferenceKey="org.eclipse.scava.plugin.annotations.apimigrationdetection.highlight"
            highlightPreferenceValue="true"
            icon="icons/icon-16-white.png"
            includeOnPreferencePage="true"
            isGoToNextNavigationTarget="true"
            isGoToNextNavigationTargetKey="org.eclipse.scava.plugin.annotations.apimigrationdetection.gotonext"
            isGoToPreviousNavigationTarget="true"
            isGoToPreviousNavigationTargetKey="org.eclipse.scava.plugin.annotations.apimigrationdetection.gotoprevious"
            label="CROSSMINER API Migration Detection"
            overviewRulerPreferenceKey="org.eclipse.scava.plugin.annotations.apimigrationdetection.overview"
            overviewRulerPreferenceValue="true"
            presentationLayer="10000"
            quickFixIcon="icons/icon-16-white.png"
            showInNextPrevDropdownToolbarAction="true"
            showInNextPrevDropdownToolbarActionKey="org.eclipse.scava.plugin.annotations.apimigrationdetection.nextprev"
            textPreferenceKey="org.eclipse.scava.plugin.annotations.apimigrationdetection.text"
            textPreferenceValue="true"
            textStylePreferenceKey="org.eclipse.scava.plugin.annotations.apimigrationdetection.textstyle"
            textStylePreferenceValue="BOX"
            verticalRulerPreferenceKey="org.eclipse.scava.plugin.annotations.apimigrationdetection.ruler"
            verticalRulerPreferenceValue="true">
      </specification>
   </extension>
